    "def printPattern(n):\n",
    "    for i in range(0, n + 1):\n",
    "        for j in range(0, i + 1):\n",
    "            print(\"*\"*j)\n",
    "\n",
    "n = int(input(\"n = \"))\n",
    "printPattern(n)"
